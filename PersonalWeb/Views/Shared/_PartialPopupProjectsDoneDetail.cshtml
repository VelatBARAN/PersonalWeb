@model PersonalWeb.Models.Entity.ProjectsDone

    <div class="row">
        <div class="col-md-12">
            <h3 class="text-center">
                @Model.ProjectName
            </h3>
        </div>

        <div class="col-md-12">
            <hr />
        </div>

        <div class="col-md-12">
            <img src="~/images/projectsdone/@Model.ImageURL" class="img-responsive img-centered">
        </div>

        <div class="col-md-12">
            <hr />
        </div>

        <div class="col-md-12">
            <p>@Html.Raw(Model.Description)</p>
        </div>
    </div>
