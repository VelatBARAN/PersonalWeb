
<!DOCTYPE HTML>
<html>
<head>
    <title>WBSoftware | Yonetim Paneli</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="WBSoftware, Mssql, Mvc5, Asp.Net, PersonalWeb, Bootstrap, Entity Framework,LinQ,Jquery,Javascript" />
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>

    <!-- Bootstrap Core CSS -->
    <link href="~/Content/AdminTemplate/css/bootstrap.css" rel='stylesheet' type='text/css' />

    <!-- Bootstrap Datepicker CSS -->
    <link href="~/Content/bootstrap-datepicker.css" rel="stylesheet" />

    <!-- Custom CSS -->
    <link href="~/Content/AdminTemplate/css/style.css" rel='stylesheet' type='text/css' />

    <!-- font-awesome icons CSS -->
    <link href="~/Content/AdminTemplate/css/font-awesome.css" rel="stylesheet">
    <!-- //font-awesome icons CSS-->
    <!-- side nav css file -->
    <link href='~/Content/AdminTemplate/css/SidebarNav.min.css' media='all' rel='stylesheet' type='text/css' />
    <!-- //side nav css file -->
    <!-- js-->
    <script src="~/Content/AdminTemplate/js/jquery-1.11.1.min.js"></script>
    <script src="~/Content/AdminTemplate/js/modernizr.custom.js"></script>
    <!-- CKEditor-->
    <script src="~/Scripts/ckeditor/ckeditor.js"></script>
    <!--webfonts-->
    <link href="https://fonts.googleapis.com/css?family=PT+Sans:400,400i,700,700i&amp;subset=cyrillic,cyrillic-ext,latin-ext" rel="stylesheet">
    <!--//webfonts-->
    <!-- chart -->
    <script src="~/Content/AdminTemplate/js/Chart.js"></script>
    <!-- //chart -->
    <!-- Metis Menu -->
    <script src="~/Content/AdminTemplate/js/metisMenu.min.js"></script>
    <script src="~/Content/AdminTemplate/js/custom.js"></script>
    <link href="~/Content/AdminTemplate/css/custom.css" rel="stylesheet">
    <!--//Metis Menu -->
    <style>
        #chartdiv {
            width: 100%;
            height: 295px;
        }
    </style>
    <!--pie-chart --><!-- index page sales reviews visitors pie chart -->
    <script src="~/Content/AdminTemplate/js/pie-chart.js" type="text/javascript"></script>
    <script type="text/javascript">

        $(document).ready(function () {
            $('#demo-pie-1').pieChart({
                barColor: '#2dde98',
                trackColor: '#eee',
                lineCap: 'round',
                lineWidth: 8,
                onStep: function (from, to, percent) {
                    $(this.element).find('.pie-value').text(Math.round(percent) + '%');
                }
            });

            $('#demo-pie-2').pieChart({
                barColor: '#8e43e7',
                trackColor: '#eee',
                lineCap: 'butt',
                lineWidth: 8,
                onStep: function (from, to, percent) {
                    $(this.element).find('.pie-value').text(Math.round(percent) + '%');
                }
            });

            $('#demo-pie-3').pieChart({
                barColor: '#ffc168',
                trackColor: '#eee',
                lineCap: 'square',
                lineWidth: 8,
                onStep: function (from, to, percent) {
                    $(this.element).find('.pie-value').text(Math.round(percent) + '%');
                }
            });


        });

    </script>

</head>
<body class="cbp-spmenu-push">
    <div class="main-content">
        <div class="cbp-spmenu cbp-spmenu-vertical cbp-spmenu-left" id="cbp-spmenu-s1">
            <!--left-fixed -navigation-->
            <aside class="sidebar-left">
                <nav class="navbar navbar-inverse">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".collapse" aria-expanded="false">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <h1><a class="navbar-brand" href="/yonetimpaneli">WBSoftware<span class="dashboard_text">Yönetim Paneli</span></a></h1>
                    </div>
                    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">

                        @if (Session["login"] != null)
                        {
                            <ul class="sidebar-menu">
                                <li class="header">SEÇENEKLER</li>
                                <li class="treeview">
                                    <a href="/yonetimpaneli">
                                        <i class="fa fa-dashboard"></i> <span>Dashboard</span>
                                    </a>
                                </li>
                                <li class="treeview">
                                    <a href="/yonetimpaneli/profil">
                                        <i class="fa fa-user"></i> <span>Profil</span>
                                    </a>
                                </li>
                                <li class="treeview">
                                    <a href="/yonetimpaneli/hakkimizda">
                                        <i class="fa fa-archive"></i> <span>Hakkımızda</span>
                                    </a>
                                </li>
                                <li class="treeview">
                                    <a href="/yonetimpaneli/hizmetler">
                                        <i class="fa fa-server"></i> <span>Hizmetler</span>
                                    </a>
                                </li>
                                <li class="treeview">
                                    <a href="/yonetimpaneli/yapilanprojeler">
                                        <i class="fa fa-th-list"></i> <span>Yapılan Projeler</span>
                                    </a>
                                </li>
                                <li class="treeview">
                                    <a href="/yonetimpaneli/iletisim">
                                        <i class="fa fa-calendar-times-o"></i> <span>İletişim</span>
                                    </a>
                                </li>
                            </ul>
                        }
                    </div>
                    <!-- /.navbar-collapse -->
                </nav>
            </aside>
        </div>
        <!--left-fixed -navigation-->
        <!-- header-starts -->
        <div class="sticky-header header-section ">
            <div class="header-left">
                <!--toggle button start-->
                <button id="showLeftPush"><i class="fa fa-bars"></i></button>
                <!--toggle button end-->
                <div class="clearfix"> </div>
            </div>

            @if (Session["login"] != null)
            {
            <div class="header-right">
                <div class="profile_details">
                    <ul>
                        <li class="dropdown profile_details_drop">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                <div class="profile_img">
                                    <span class="prfil-img"><img src="images/2.jpg" alt=""> </span>
                                    <div class="user-name">
                                        <p>@Session["login"]</p>
                                        <span>Administrator</span>
                                    </div>
                                    <i class="fa fa-angle-down lnr"></i>
                                    <i class="fa fa-angle-up lnr"></i>
                                    <div class="clearfix"></div>
                                </div>
                            </a>
                            <ul class="dropdown-menu drp-mnu">
                                <li> <a href="/Admin/ShowProfile"><i class="fa fa-suitcase"></i> Profil</a> </li>
                                <li> <a href="/Admin/LogOut"><i class="fa fa-sign-out"></i> Çıkış</a> </li>
                            </ul>
                        </li>
                    </ul>
                </div>
                <div class="clearfix"> </div>
            </div>
            }
            <div class="clearfix"> </div>
        </div>
        <!-- //header-ends -->
        <!-- main content start-->
        <div id="page-wrapper">

            @RenderBody()
            <!--footer-->
            <div class="footer">
                <p>&copy; @DateTime.Now.Year WBSoftware . Tüm Hakları Saklıdır | Bilgisayar Mühendisi <a href="" target="_blank">Welat BARAN</a>tarafından hazırlandı.</p>
            </div>
            <!--//footer-->
        </div>

        <!-- Classie --><!-- for toggle left push menu script -->
        <script src="~/Content/AdminTemplate/js/classie.js"></script>
        <script>
            var menuLeft = document.getElementById('cbp-spmenu-s1'),
                showLeftPush = document.getElementById('showLeftPush'),
                body = document.body;

            showLeftPush.onclick = function () {
                classie.toggle(this, 'active');
                classie.toggle(body, 'cbp-spmenu-push-toright');
                classie.toggle(menuLeft, 'cbp-spmenu-open');
                disableOther('showLeftPush');
            };


            function disableOther(button) {
                if (button !== 'showLeftPush') {
                    classie.toggle(showLeftPush, 'disabled');
                }
            }
        </script>
        <!-- //Classie --><!-- //for toggle left push menu script -->
        <!--scrolling js-->
        <script src="~/Content/AdminTemplate/js/jquery.nicescroll.js"></script>
        <script src="~/Content/AdminTemplate/js/scripts.js"></script>
        <!--//scrolling js-->
        <!-- side nav js -->
        <script src='~/Content/AdminTemplate/js/SidebarNav.min.js' type='text/javascript'></script>
        <script>
            $('.sidebar-menu').SidebarNav()
        </script>
        <!-- //side nav js -->
        <!-- Bootstrap Core JavaScript -->
        <script src="~/Content/AdminTemplate/js/bootstrap.js"></script>

        <!-- Bootstrap Datepicker JavaScript -->
        <script src="~/Scripts/bootstrap-datepicker.js"></script>
        <!-- //Bootstrap Core JavaScript -->
    </div>
</body>
</html>